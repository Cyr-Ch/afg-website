    <!-- Hero Section -->
    <section class="hero">
        <div class="hero-background">
            <!-- Placeholder: Full-screen image of Munich meetup with diverse young professionals networking, teal color overlay -->
            <div class="hero-overlay"></div>
        </div>
        <div class="container">
            <div class="hero-content">
                <h1 class="hero-title">AI for Good | Munich Hub</h1>
                <p class="hero-subtitle">Empowering 18–30 year‐olds to build AI solutions for the SDGs</p>
                <a href="#join" class="btn btn-primary btn-large">Join Our Community</a>
            </div>
        </div>
    </section>

    <!-- What We Do Section -->
    <section class="what-we-do section">
        <div class="container">
            <h2 class="section-title">What We Do</h2>
            <div class="grid three-columns">
                <div class="card">
                    <div class="card-icon">
                        <!-- Placeholder: Icon representing community/networking -->
                        <img src="placeholder-community-icon.svg" alt="Community Building">
                    </div>
                    <h3 class="card-title">Community Building</h3>
                    <p class="card-description">Connect, collaborate, and amplify young voices in AI.</p>
                </div>
                <div class="card">
                    <div class="card-icon">
                        <!-- Placeholder: Icon representing innovation/lab/lightbulb -->
                        <img src="placeholder-innovation-icon.svg" alt="Innovation Labs">
                    </div>
                    <h3 class="card-title">Innovation Labs</h3>
                    <p class="card-description">Prototype solutions tackling climate, health & education.</p>
                </div>
                <div class="card">
                    <div class="card-icon">
                        <!-- Placeholder: Icon representing global stage/world -->
                        <img src="placeholder-global-icon.svg" alt="Global Stage">
                    </div>
                    <h3 class="card-title">Global Stage</h3>
                    <p class="card-description">Showcase at WEF, AI House Davos, and more.</p>
                </div>
            </div>
        </div>
    </section>

    <!-- Impact Timeline Section -->
    <section class="impact-timeline section">
        <div class="container">
            <h2 class="section-title">Our Impact Timeline</h2>
            <div class="timeline">
                <div class="timeline-item">
                    <div class="timeline-marker"></div>
                    <div class="timeline-content">
                        <h3 class="timeline-title">Spring 2023</h3>
                        <p class="timeline-description">Hub founded</p>
                    </div>
                </div>
                <div class="timeline-item">
                    <div class="timeline-marker"></div>
                    <div class="timeline-content">
                        <h3 class="timeline-title">Oct 2023/24</h3>
                        <p class="timeline-description">Oktoberfest meetups</p>
                    </div>
                </div>
                <div class="timeline-item">
                    <div class="timeline-marker"></div>
                    <div class="timeline-content">
                        <h3 class="timeline-title">Jan 2024/25</h3>
                        <p class="timeline-description">Delegations to AI House Davos</p>
                    </div>
                </div>
                <div class="timeline-item">
                    <div class="timeline-marker"></div>
                    <div class="timeline-content">
                        <h3 class="timeline-title">Jun 2025</h3>
                        <p class="timeline-description">Officially part of AI for Good</p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Featured Projects Section -->
    <section class="featured-projects section">
        <div class="container">
            <h2 class="section-title">Featured Projects</h2>
            <div class="carousel">
                <div class="carousel-container">
                    <div class="project-card">
                        <!-- Placeholder: Screenshot of climate AI project dashboard -->
                        <img src="placeholder-project1.jpg" alt="Climate AI Project" class="project-image">
                        <div class="project-content">
                            <h3 class="project-title">Climate AI Monitor</h3>
                            <p class="project-description">Real-time environmental data analysis for sustainable city planning.</p>
                        </div>
                    </div>
                    <div class="project-card">
                        <!-- Placeholder: Screenshot of healthcare AI application -->
                        <img src="placeholder-project2.jpg" alt="Healthcare AI Project" class="project-image">
                        <div class="project-content">
                            <h3 class="project-title">Health Access AI</h3>
                            <p class="project-description">Improving healthcare accessibility in underserved communities.</p>
                        </div>
                    </div>
                    <div class="project-card">
                        <!-- Placeholder: Screenshot of education AI platform -->
                        <img src="placeholder-project3.jpg" alt="Education AI Project" class="project-image">
                        <div class="project-content">
                            <h3 class="project-title">EduBridge AI</h3>
                            <p class="project-description">Personalized learning experiences for students with disabilities.</p>
                        </div>
                    </div>
                </div>
                <button class="carousel-btn carousel-prev" aria-label="Previous project">&larr;</button>
                <button class="carousel-btn carousel-next" aria-label="Next project">&rarr;</button>
            </div>
        </div>
    </section>

    <!-- Get Involved Section -->
    <section class="get-involved section" id="join">
        <div class="container">
            <h2 class="section-title">Get Involved</h2>
            <div class="grid two-columns">
                <div class="involvement-card">
                    <h3 class="card-title">Become a Member</h3>
                    <ul class="benefits-list">
                        <li>Access to exclusive workshops and events</li>
                        <li>Mentorship from AI industry leaders</li>
                        <li>Opportunity to pitch at global conferences</li>
                        <li>Collaborative project development</li>
                        <li>Networking with like-minded innovators</li>
                    </ul>
                    <a href="contact.html?type=membership" class="btn btn-primary">Apply Now</a>
                </div>
                <div class="involvement-card">
                    <h3 class="card-title">Host an Event</h3>
                    <p class="card-description">Partner with us to organize impactful AI events in Munich. From technical workshops to community panels, we're always looking for collaborative opportunities.</p>
                    <a href="contact.html?type=partnership" class="btn btn-outline">Contact Us</a>
                </div>
            </div>
        </div>
    </section>

